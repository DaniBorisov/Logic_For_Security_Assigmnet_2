CREATE TABLE public.Patient
    (
        ID int NOT NULL UNIQUE,
        Name text,
        CPR int NOT NULL
    );

CREATE TABLE public.Test_table
(
    ID int NOT NULL PRIMARY KEY,
    type text,
    date date NOT NULL,
    Doctor text,
    p_id cid NOT NULL,
    result text

);

DROP TABLE public.patient

CREATE TABLE public.Vaccination_table
(
    ID int NOT NULL PRIMARY KEY,
    type text,
    date date NOT NULL,
    Doctor text,
    p_id cid NOT NULL,
    shots int

);

CREATE TABLE public.Appointment_Vacc
(
    ID int NOT NULL PRIMARY KEY,
    p_id cid NOT NULL,
    date date NOT NULL
);
CREATE TABLE public.Appointment_Test
(
    ID int NOT NULL PRIMARY KEY,
    p_id cid NOT NULL,
    date date NOT NULL
);

